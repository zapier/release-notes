{
  "name": "release-notes",
  "version": "1.0.1",
  "description": "Generate release notes from recent pull requests",
  "main": "lib/index.js",
  "bin": {
    "release-notes": "lib/cli.js"
  },
  "files": [
    "/lib/*.js",
    "/lib/index.d.ts",
    "!/lib/interfaces.js"
  ],
  "dependencies": {
    "commander": "2.15.1",
    "got": "8.3.1",
    "lodash": "4.17.11"
  },
  "repository": "zapier/release-notes",
  "devDependencies": {
    "@types/dotenv": "4.0.3",
    "@types/got": "8.3.1",
    "@types/jest": "22.2.3",
    "@types/lodash": "4.14.108",
    "dotenv": "6.0.0",
    "jest": "22.4.3",
    "ts-jest": "22.4.6",
    "tslint": "5.10.0",
    "tslint-config-prettier": "1.13.0",
    "typescript": "2.9.1"
  },
  "scripts": {
    "test": "jest",
    "posttest": "yarn lint",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "preversion": "yarn build",
    "build": "tsc",
    "release": "npx np"
  },
  "keywords": [
    "typescript",
    "npm",
    "release-notes",
    "github"
  ],
  "author": "David Brownman <beamneocube@gmail.com> (https://davidbrownman.com)",
  "license": "MIT",
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "typings": "lib/index.d.ts",
  "jest": {
    "transform": {
      ".+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
